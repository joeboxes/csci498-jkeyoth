/*
Array.jack
*/
class Array{
	/** Constructs a new Array of the given size. */
	function Array new(int size) {
		if(size<0){// size must be positive
			do Sys.error(2);
		}
		return Memory.alloc(size);
	}
	/** De-allocates the array and frees its space. */
	method void dispose(){
		do Memory.deAlloc(this);
		return;
	}
}
